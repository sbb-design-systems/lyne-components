{
  "name": "@sbb-esta/lyne-elements",
  "version": "4.0.0",
  "description": "Lyne Design System",
  "keywords": [
    "design system",
    "web components",
    "lit",
    "storybook"
  ],
  "author": "Lyne Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sbb-design-systems/lyne-components.git"
  },
  "bugs": {
    "url": "https://github.com/sbb-design-systems/lyne-components/issues"
  },
  "type": "module",
  "private": true,
  "scripts": {
    "postinstall": "yarn playwright install",
    "build:elements": "npm-run-all --sequential build:elements:* build:elements-experimental:*",
    "build:elements:production": "vite build --config src/elements/vite.config.ts",
    "build:elements:development": "cross-env NODE_ENV=development vite build --mode development --config src/elements/vite.config.ts",
    "build:elements-experimental:production": "vite build --config src/elements-experimental/vite.config.ts",
    "build:elements-experimental:development": "cross-env NODE_ENV=development vite build --mode development --config src/elements-experimental/vite.config.ts",
    "build:react": "npm-run-all --sequential build:react:* build:react-experimental:*",
    "build:react:production": "vite build --config src/react/vite.config.ts",
    "build:react:development": "cross-env NODE_ENV=development vite build --mode development --config src/react/vite.config.ts",
    "build:react-experimental:production": "vite build --config src/react-experimental/vite.config.ts",
    "build:react-experimental:development": "cross-env NODE_ENV=development vite build --mode development --config src/react-experimental/vite.config.ts",
    "build:calculate-stats": "node scripts/calculate-stats.ts",
    "build:size-check": "npm-run-all --sequential build:elements:production build:elements-experimental:production build:calculate-stats size-check",
    "build:storybook": "storybook build --quiet --output-dir dist/storybook --stats-json",
    "build:visual-regression-app": "vite build --config src/visual-regression-app/vite.config.ts",
    "build": "npm-run-all --sequential build:elements build:react build:storybook build:calculate-stats",
    "docs": "npm-run-all --sequential docs:manifest docs:to-md",
    "docs:manifest": "npm-run-all --sequential docs:manifest:*",
    "docs:manifest:elements": "custom-elements-manifest analyze --config tools/manifest/elements-custom-elements-manifest.config.ts",
    "docs:manifest:elements-experimental": "custom-elements-manifest analyze --config tools/manifest/elements-experimental-custom-elements-manifest.config.ts",
    "docs:to-md": "node tools/docs/docs_generate.ts",
    "format": "prettier \"**/*\" --write --ignore-unknown",
    "generate": "node tools/generate-component/index.ts",
    "integrity": "npm-run-all format docs",
    "lint": "npm-run-all --sequential lint:*",
    "lint:ts": "eslint \"**/*.ts\"",
    "lint:js": "eslint \"**/*.js\"",
    "lint:yml": "eslint \"**/*.{yml,yaml}\"",
    "lint:styles": "stylelint \"**/*.scss\"",
    "lint:lit": "yarn lit-analyzer \"src/**/*.ts\"",
    "lint:circular-imports": "madge --circular --ts-config ./tsconfig.json --extensions ts ./src",
    "lint:tsc": "npm-run-all --sequential lint:tsc:*",
    "lint:tsc:components": "tsc --noEmit --project src/elements/tsconfig.json",
    "lint:tsc:components-spec": "tsc --noEmit --project src/elements/tsconfig.spec.json",
    "lint:tsc:components-experimental": "tsc --noEmit --project src/elements-experimental/tsconfig.json",
    "lint:tsc:components-experimental-spec": "tsc --noEmit --project src/elements-experimental/tsconfig.spec.json",
    "lint:tsc:visual-regression-app": "tsc --noEmit --project src/visual-regression-app/tsconfig.json",
    "lint:tsc:scripts": "tsc --noEmit --project scripts/tsconfig.json",
    "lint:tsc:tools": "tsc --noEmit --project tools/tsconfig.json",
    "size-check": "node scripts/compare-stats.ts",
    "start": "storybook dev -p 6006",
    "start:visual-regression-app": "vite --config src/visual-regression-app/vite.config.ts",
    "test": "yarn -s wtr --coverage",
    "test:snapshot": "yarn test:csr --ci --update-snapshots",
    "test:csr": "yarn -s wtr --group default",
    "test:visual-regression": "yarn -s wtr --visual-regression --all-browsers",
    "test:playwright-server": "node tools/web-test-runner/start-playwright-server.ts",
    "wtr": "node node_modules/@web/test-runner/dist/bin.js --config=web-test-runner.config.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "lit": "3.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@custom-elements-manifest/analyzer": "0.11.0",
    "@custom-elements-manifest/to-markdown": "0.1.0",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@lit-labs/observers": "2.1.0",
    "@lit-labs/router": "0.1.4",
    "@lit-labs/testing": "0.2.8",
    "@lit/react": "1.0.8",
    "@open-wc/lit-helpers": "0.7.0",
    "@open-wc/testing": "4.0.0",
    "@sbb-esta/lyne-design-tokens": "2.0.0",
    "@storybook/addon-a11y": "10.1.11",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-links": "10.1.11",
    "@storybook/icons": "2.0.1",
    "@storybook/web-components-vite": "10.1.11",
    "@types/chai": "4.3.20",
    "@types/mocha": "10.0.10",
    "@types/node": "24.10.9",
    "@types/react": "19.2.9",
    "@typescript-eslint/eslint-plugin": "8.53.1",
    "@typescript-eslint/parser": "8.53.1",
    "@web/test-runner": "0.20.2",
    "@web/test-runner-commands": "0.9.0",
    "@web/test-runner-playwright": "0.11.1",
    "@web/test-runner-visual-regression": "0.9.0",
    "cross-env": "10.1.0",
    "custom-elements-manifest": "2.1.0",
    "date-fns": "4.1.0",
    "esbuild": "0.27.2",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-lit": "2.1.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-storybook": "10.1.11",
    "eslint-plugin-yml": "3.0.0",
    "globals": "17.0.0",
    "husky": "9.1.7",
    "jiti": "2.6.1",
    "lint-staged": "16.2.7",
    "lit-analyzer": "2.0.3",
    "madge": "8.0.0",
    "npm-run-all2": "8.0.4",
    "nyc": "17.1.0",
    "octokit": "5.0.5",
    "playwright": "1.56.1",
    "postcss": "8.5.6",
    "postcss-value-parser": "4.2.0",
    "prettier": "3.8.0",
    "react": "19.2.3",
    "rollup-plugin-postcss-lit": "2.2.0",
    "sass": "1.97.2",
    "sinon": "21.0.1",
    "storybook": "10.1.11",
    "stylelint": "17.0.0",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "17.0.0",
    "stylelint-scss": "7.0.0",
    "ts-lit-plugin": "2.0.2",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
    "urlpattern-polyfill": "10.1.0",
    "vite": "7.3.1",
    "vite-plugin-dts": "4.5.4"
  },
  "resolutions": {
    "@types/node": "24.10.9",
    "@webcomponents/template-shadowroot": "0.2.1",
    "@web/dev-server-core": "0.7.1",
    "jackspeak": "2.1.1",
    "lit": "3.3.2",
    "playwright": "1.56.1"
  },
  "prettier": {
    "singleQuote": true,
    "endOfLine": "lf",
    "printWidth": 100
  },
  "lint-staged": {
    "!(tools/generate-component/**/*)*.{js,ts,yaml,yml}": [
      "eslint --fix --no-ignore"
    ],
    "*.scss": [
      "stylelint --fix=true"
    ],
    "*.{js,ts,css,scss,json,md,html,yaml,yml}": [
      "prettier --write"
    ]
  },
  "madge": {
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      }
    }
  }
}
