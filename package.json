{
  "name": "lyne-test",
  "version": "9.7.4",
  "description": "Lyne Design System",
  "keywords": [
    "design system",
    "web components",
    "stencil",
    "storybook"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/lyne-components/",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build:stencil": "stencil build --ci --prod --docs",
    "build:storybook": "build-storybook --quiet",
    "build": "npm-run-all --sequential build:stencil build:storybook",
    "chromatic": "chromatic --storybook-build-dir=./storybook-static --exit-zero-on-changes",
    "chromatic:master": "chromatic --storybook-build-dir=./storybook-static --exit-zero-on-changes --auto-accept-changes",
    "generate": "stencil generate",
    "lint": "npm run lint:ts && npm run lint:js && npm run lint:yml",
    "lint:ts": "eslint ./ --ext .ts,.tsx",
    "lint:js": "eslint ./ --ext .js",
    "lint:yml": "eslint ./ --ext .yml,.yaml",
    "semantic-release": "semantic-release",
    "start:stencil": "stencil build --prod --watch --docs",
    "start:storybook": "start-storybook -p 6006 -s dist",
    "start": "npm-run-all --sequential build:stencil --parallel start:stencil start:storybook",
    "test:dev": "stencil test --spec --e2e --coverage",
    "test:prod": "stencil test --prod --spec --e2e --coverage",
    "test:watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "~7.12.10",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "~7.12.12",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@stencil/core": "^2.3.0",
    "@stencil/sass": "^1.3.1",
    "@storybook/addon-a11y": "~6.1.15",
    "@storybook/addon-actions": "~6.1.15",
    "@storybook/addon-docs": "~6.1.15",
    "@storybook/addon-knobs": "~6.1.15",
    "@storybook/html": "~6.1.15",
    "@typescript-eslint/eslint-plugin": "~4.14.0",
    "@typescript-eslint/parser": "~4.14.0",
    "axios": "~0.21.1",
    "babel-loader": "^8.1.0",
    "eslint": "~7.19.0",
    "@types/jest": "26.0.20",
    "@types/puppeteer": "5.4.3",
    "chromatic": "~5.6.1",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-yaml": "^0.3.0",
    "husky": "~4.3.8",
    "jest": "~26.6.1",
    "jest-cli": "26.6.3",
    "jsx-dom": "^6.4.15",
    "lyne-design-tokens": "^3.2.6",
    "lyne-helper-eslint-config": "~1.0.1",
    "lyne-helper-trigger-travis": "~1.0.3",
    "lyne-icons": "^4.9.0",
    "minimist": "~1.2.5",
    "npm-run-all": "^4.1.5",
    "semantic-release": "~17.3.7",
    "semantic-release-slack-bot": "~2.1.0",
    "puppeteer": "7.0.1",
    "shelljs": "^0.8.4",
    "simple-git": "~2.34.2",
    "stencil-inline-svg": "~1.0.1",
    "typescript": "~4.1.3",
    "yargs": "~16.2.0"
  },
  "author": "Lyne Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lyne-design-system/lyne-components.git"
  },
  "bugs": {
    "url": "https://github.com/lyne-design-system/lyne-components/issues"
  }
}
